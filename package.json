{
    "name": "budget-feed-reader",
    "version": "0.1.0",
    "type": "module",
    "enginges": {
        "node": ">= 20.19"
    },
    "scripts": {
        "dev:backend": "nodemon backend/src/server.js",
        "dev:frontend": "vite",
        "dev": "concurrently --names \"BACKEND,FRONTEND\" -c \"yellow,magenta\" \"npm run dev:backend\" \"npm run dev:frontend\"",
        "build": "vite build",
        "preview": "vite preview",
        "start": "node backend/src/server.js",
        "format": "prettier --write .",
        "lint": "prettier --check ."
    },
    "license": "MIT",
    "devDependencies": {
        "concurrently": "^9.2.1",
        "morgan": "^1.10.1",
        "nodemon": "^3.1.10",
        "prettier": "3.6.2",
        "vite": "^7.1.9"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.14",
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "helmet": "^8.1.0",
        "rss2html": "^1.0.7",
        "tailwindcss": "^4.1.14"
    }
}
